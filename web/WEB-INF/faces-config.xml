<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

   <managed-bean eager="true">
        <managed-bean-name>serverTime</managed-bean-name>
        <managed-bean-class>com.ServerTimeBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean eager="false">
        <managed-bean-name>dotsColl</managed-bean-name>
        <managed-bean-class>com.DotsCollectionBean</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>

    <managed-bean eager="false">
        <managed-bean-name>dotMake</managed-bean-name>
        <managed-bean-class>com.DotMaker</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>

    <managed-bean eager="false">
        <managed-bean-name>resizeDot</managed-bean-name>
        <managed-bean-class>com.ResizeBean</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>

    <managed-bean eager="false">
        <managed-bean-name>clickDot</managed-bean-name>
        <managed-bean-class>com.AreaClickedDots</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>

    <managed-bean eager="false">
        <managed-bean-name>formDot</managed-bean-name>
        <managed-bean-class>com.FormClickedDots</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>

</faces-config>
